<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
    <header>
        <nav class="bg-[#0f1b09]">
            <ul class="flex gap-20 w-9/12 m-auto items-center">
                <li><a class="text-2xl text-[#9fc53a]" href="{{ path('app_home') }}"><img class="w-24" src="{{asset("uploads/logo_zombie_fit.png")}}"></a></li>
                <li><a class="text-2xl text-[#9fc53a] font-semibold" href="{{ path('app_register') }}">Register</a></li>
                <li><a class="text-2xl text-[#9fc53a] font-semibold" href="{{ path('app_login') }}">Login</a></li>
                <li></li>
            </ul>
        </nav>
    </header>

    <main>
        {% block body %}{% endblock %}
    </main>

    <footer>
        <div class="bg-[#0f1b09]">
            <div class="w-9/12 m-auto flex">
                <a class="text-2xl text-[#9fc53a]" href="{{ path('app_home') }}"><img class="w-24" src="{{asset("uploads/logo_zombie_fit.png")}}"></a>
                <div>
                </div>
            </div>
        </div>
    </footer>

    </body>
</html>
